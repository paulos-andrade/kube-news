pipeline {
    agent any

    stages {
        stage ('Build Docker Image') {
            steps{
                script{
                    dockerapp = docker.build("averdade/kube-news:${env.BUILD_ID}", '-f ./src/Dockerfile ./src')
                }
            }
        }
         stage ('Push Docker Image') {
            steps{
                script{
                    docker.withRegistry('','dockerhub')
                }
            }
        }
    }
 }